<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="600" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.psymk6.controllers.ViewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <StackPane>
        <Rectangle fill="#1a1a1a" height="600.0" width="800.0" />

        <VBox alignment="CENTER" spacing="20">
            <Text fill="#0066ff" text="SnakeGame">
                <font>
                    <Font name="Forte" size="91.0" />
                </font>
            <effect>
               <Bloom />
            </effect>
            </Text>
            <Button onAction="#onEnterButtonClick" style="-fx-font-size: 28; -fx-background-color: #0066ff; -fx-text-fill: black; -fx-font-family: consolas;" text="Start Game" textAlignment="CENTER" textFill="#1a1a1a">
                <cursor>
                   <Cursor fx:constant="CROSSHAIR" />
                </cursor>
            <font>
               <Font name="Franklin Gothic Demi" size="16.0" />
            </font>
            <effect>
               <Bloom />
            </effect>
                Hi
            </Button>
        </VBox>
    </StackPane>
</VBox>
